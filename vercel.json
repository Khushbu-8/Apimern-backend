{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1" 
      }
    ],
    "env": {
      "REACT_APP_API_URL": "https://apimern-backend.vercel.app/api/v1" 
    },
    "static": {
      "directory": "build"
    }
  }